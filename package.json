{
  "name": "babel-plugin-react-scoped-css",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "cd examples/simple && npm start",
    "pub": "yarn lerna publish",
    "test": "jest",
    "test:update-snapshot": "jest --updateSnapshot",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "format": "prettier --write \"packages/**/*.js\"",
    "v": "yarn lerna version --conventional-commits -y"
  },
  "repository": "https://github.com/gaoxiaoliangz/react-scoped-css",
  "devDependencies": {
    "jest": "^29.5.0",
    "lerna": "^6.6.2",
    "prettier": "^1.19.1"
  },
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "dependencies": {
    "css-loader": "6.7.4",
    "react-scripts": "5.0.1"
  }
}
